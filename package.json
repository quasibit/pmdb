{
  "name": "pmdb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "husky install",
    "commit": "git-cz",
    "commitlint": "commitlint --to=HEAD",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,md,css,scss,yaml,yml}\"",
    "eslint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "markdownlint": "markdownlint \"**/*.md\"",
    "stylelint": "stylelint \"./**/*.ts\" \"./**/*.tsx\" --cache",
    "lint": "run-p --continue-on-error lint:*",
    "lint:prettier": "yarn run prettier --check",
    "lint:eslint": "yarn run eslint",
    "lint:json": "eslint . --cache --cache-location .eslintjsoncache --ext .json --no-eslintrc --config .eslintjsonrc",
    "lint:markdownlint": "yarn run markdownlint",
    "lint:stylelint": "yarn run stylelint",
    "lint:typescript": "tsc --noEmit",
    "format": "run-s format:*",
    "format:prettier": "yarn run prettier --write",
    "format:eslint": "yarn run lint:eslint --fix",
    "format:markdownlint": "yarn run markdownlint --fix",
    "format:stylelint": "yarn run stylelint --fix",
    "test": "jest",
    "test-ci": "jest --ci",
    "coverage": "jest --coverage",
    "generate": "plop"
  },
  "dependencies": {
    "linaria": "^3.0.0-beta.1",
    "next": "10.0.6",
    "next-linaria": "^0.10.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.8",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "@wemake-services/eslint-config-jest": "^0.2.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "commitizen": "^4.2.3",
    "eslint": "^7.19.0",
    "eslint-config-hardcore": "^15.3.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-next-dynamic": "^1.0.1",
    "lint-staged": "^10.5.3",
    "markdownlint-cli": "^0.26.0",
    "node-plop": "^0.26.2",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "stylelint": "^13.9.0",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-declaration-strict-value": "^1.7.7",
    "stylelint-high-performance-animation": "^1.5.2",
    "stylelint-no-indistinguishable-colors": "^1.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-selector-tag-no-without-class": "^2.0.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
